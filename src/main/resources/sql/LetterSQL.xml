<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="letter">
	
	<!-- 쪽지 리스트 조회 -->
	<select id="selectLetterList" resultType="Letter">
		SELECT LETTER_SEQ
			  ,RECEIVER_ID
			  ,SENDER_ID
			  ,CONTENT
			  ,SEND_DATE
			  ,CHECK_YN
			  ,CHECK_DATE
			  ,DELETE_YN
		FROM LETTER
	</select>

</mapper>